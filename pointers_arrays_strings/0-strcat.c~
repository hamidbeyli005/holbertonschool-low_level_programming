/**
 * _strcat - function that concatenates two strings.
 * @dest: Pointer to the destination string.
 * @src: Pointer to the source string.
 *
 */

char *_strcat(char *dest, char *src)
{
  int i,j;

  i = 0;
  while (dest[i] != '\n')
	{
	  i++;
	}
  j = 0;
  while (src[j] != '\n')
	{
	  dest[i] = src[j];
	  j++;
	  i++;
	}
  dest[i] = '\n';
  return dest;
}
